{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "tsml/meetings",
  "title": "Meetings",
  "description": "12 Step Meeting List blocks",
  "category": "embed",
  "icon": "groups",
  "attributes": {
    "blockType": {
      "type": "string",
      "default": "tsml_ui"
    },
    "count": {
      "type": "string",
      "default": "5"
    },
    "message": {
      "type": "string",
      "default": "There are no upcoming meetings today. Please check back tomorrow."
    }
  },
  "supports": {
    "html": false,
    "align": true,
    "anchor": true,
    "className": true,
    "typography": {
      "__experimentalFontStyle": true,
      "__experimentalFontWeight": true,
      "__experimentalTextTransform": true,
      "__experimentalDefaultControls": {
        "fontAppearance": true,
        "textTransform": true
      },
      "spacing": {
        "margin": true,
        "padding": true
      }
    }
  },
  "textdomain": "12-step-meeting-list",
  "render": "file:./render.php",
  "editorScript": "file:./meetings.js"
}